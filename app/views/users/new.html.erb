<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for @user do |f| %>

  <%= f.label :name, 'Name:' %>
  <%= f.text_field :name %>

  <%= f.label :email, 'Email:' %>
  <%= f.text_field :email %>

  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password %>

  <%= f.submit %>

<%end  %>
