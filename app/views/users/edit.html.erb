<%= form_for @user do |f| %>

  <%= f.label :name, 'Name:' %>
  <%= f.text_field :name %>

  <%= f.label :user_name, 'User Name' %>
  <%= f.text_field :user_name, required: true %>

  <%= f.label :email, 'Email:' %>
  <%= f.text_field :email %>


  <%= f.label :name, 'Bio:' %>
  <%= f.text_field :bio %>

  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password %>



  <%= f.submit %>

<%end  %>
